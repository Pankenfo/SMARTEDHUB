<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smartedhub_generator.mapper.QuestionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.smartedhub_generator.pojo.Question">
        <id column="question_id" property="questionId" />
        <result column="question_detail" property="questionDetail" />
        <result column="question_title" property="questionTitle" />
        <result column="question_date" property="questionDate" />
        <result column="teacher_id" property="teacherId" />
        <result column="validity" property="validity" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        question_id, question_detail, question_title, question_date, teacher_id, validity
    </sql>

</mapper>
